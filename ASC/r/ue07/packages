is.installed <- function(mypkg) is.element(mypkg, installed.packages()[,1])
if (!is.installed("data.table")) install.packages("data.table")
if (!is.installed("plyr")) install.packages("plyr")
if (!is.installed("tibble"))install.packages("tibble")
if (!is.installed("dplyr")) install.packages("dplyr")
if (!is.installed("tidyr")) install.packages("tidyr")
if (!is.installed("microbenchmark"))install.packages("microbenchmark")

library("data.table")
library("plyr")
library("tibble")
library("dplyr")
library("tidyr")
library("microbenchmark")
library("ggplot2")
